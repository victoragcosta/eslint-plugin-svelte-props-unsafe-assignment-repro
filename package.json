{
  "name": "keyboard-visualizer",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint:eslint": "eslint .",
    "format:eslint": "eslint . --fix",
    "lint:prettier": "prettier . --check",
    "format:prettier": "prettier . --write",
    "lint": "$npm_execpath run lint:eslint; err1=$?; $npm_execpath run lint:prettier && exit $err1",
    "format": "$npm_execpath run format:eslint; err1=$?; $npm_execpath run format:prettier && exit $err1"
  },
  "devDependencies": {
    "@eslint/js": "9.5.0",
    "@sveltejs/adapter-auto": "^3.2.2",
    "@sveltejs/kit": "^2.5.17",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "@typescript-eslint/parser": "^8.0.0-alpha.30",
    "eslint": "^9.5.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-svelte": "^2.40.0",
    "globals": "^15.6.0",
    "prettier": "^3.3.2",
    "prettier-plugin-svelte": "^3.2.5",
    "svelte": "^5.0.0-next.160",
    "svelte-check": "^3.6.0",
    "svelte-eslint-parser": "^0.39.1",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.0.0-alpha.30",
    "vite": "^5.3.1"
  },
  "type": "module",
  "trustedDependencies": [
    "@sveltejs/kit",
    "esbuild",
    "svelte-preprocess"
  ]
}
